package classes;

import java.sql.Date;

public class Boleta {
		
	private int Cod_Boleta;
	private Mozo mozo;
	private Cajero cajero;
	private Date fecha;
	private Date hora;
	private double TotalPagar;
	private double VueltoPagar;
	private double dinero;
	private Cliente cliente;
	private Mesa mesa;
	private Boolean pagado;
	public Boleta(int cod_Boleta, Mozo mozo, Cajero cajero, Date fecha, Date hora, double totalPagar,
			double vueltoPagar, double dinero, Cliente cliente, Mesa mesa, Boolean pagado) {
		Cod_Boleta = cod_Boleta;
		this.mozo = mozo;
		this.cajero = cajero;
		this.fecha = fecha;
		this.hora = hora;
		TotalPagar = totalPagar;
		VueltoPagar = vueltoPagar;
		this.dinero = dinero;
		this.cliente = cliente;
		this.mesa = mesa;
		this.pagado = pagado;
	}
	public int getCod_Boleta() {
		return Cod_Boleta;
	}
	public void setCod_Boleta(int cod_Boleta) {
		Cod_Boleta = cod_Boleta;
	}
	public Mozo getMozo() {
		return mozo;
	}
	public void setMozo(Mozo mozo) {
		this.mozo = mozo;
	}
	public Cajero getCajero() {
		return cajero;
	}
	public void setCajero(Cajero cajero) {
		this.cajero = cajero;
	}
	public Date getFecha() {
		return fecha;
	}
	public void setFecha(Date fecha) {
		this.fecha = fecha;
	}
	public Date getHora() {
		return hora;
	}
	public void setHora(Date hora) {
		this.hora = hora;
	}
	public double getTotalPagar() {
		return TotalPagar;
	}
	public void setTotalPagar(double totalPagar) {
		TotalPagar = totalPagar;
	}
	public double getVueltoPagar() {
		return VueltoPagar;
	}
	public void setVueltoPagar(double vueltoPagar) {
		VueltoPagar = vueltoPagar;
	}
	public double getDinero() {
		return dinero;
	}
	public void setDinero(double dinero) {
		this.dinero = dinero;
	}
	public Cliente getCliente() {
		return cliente;
	}
	public void setCliente(Cliente cliente) {
		this.cliente = cliente;
	}
	public Mesa getMesa() {
		return mesa;
	}
	public void setMesa(Mesa mesa) {
		this.mesa = mesa;
	}
	public Boolean getPagado() {
		return pagado;
	}
	public void setPagado(Boolean pagado) {
		this.pagado = pagado;
	}
	
	
	
	
	
}
