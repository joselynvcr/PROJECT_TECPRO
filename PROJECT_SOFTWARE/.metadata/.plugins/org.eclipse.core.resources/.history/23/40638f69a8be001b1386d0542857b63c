package saborea.model.bussiness.DTO;

import javax.swing.JOptionPane;

import PaqueteLog.Administrador;
import PaqueteLog.Cajero;
import PaqueteLog.Mozo;

public class VentanaTipoCargoBE {
	

	String TipoCargo
	
	
	if(datosCredenciales.equals("cajero")){				
		Cajero m= new Cajero(Cred);		
		m.setVisible(true);
	}else if(datosCredenciales.equals("mozo")){				
		Mozo m= new Mozo(Cred);				
		m.setVisible(true);
	}else if(datosCredenciales.equals("administrador")){				
		Administrador a= new Administrador(Cred);		
		a.setVisible(true);
	}else{				
		JOptionPane.showMessageDialog(null, "NO SE ENCONTRÓ NINGUN TIPO DE CARGO ","ADVERTENCIA", JOptionPane.WARNING_MESSAGE);		
	}	
	
	
	
	
}
